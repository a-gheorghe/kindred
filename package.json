{
  "name": "kindred-talent",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "setup-db": "node_modules/.bin/sequelize db:create && node_modules/.bin/sequelize db:migrate && node database/sync.js",
    "clear-db": "node_modules/.bin/sequelize db:seed:undo:all",
    "drop-db": "node_modules/.bin/sequelize db:drop",
    "dev": "concurrently -k \"npm run frontend\" \"npm run backend\"",
    "frontend": "webpack -d --watch",
    "backend": "nodemon server.js",
    "win_start": "npm run build && node server.js",
    "start": "npm run dev",
    "test": "mocha --compilers js:babel-core/register ./test/**/*.js --require ignore-styles",
    "lint": "npm run lint:js",
    "lint:js": "eslint -- frontend backend database models migrations config test",
    "lint:errors": "eslint --quiet -- frontend backend database models migrations config test",
    "lint:staged": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --quiet --fix",
      "git add"
    ]
  },
  "pre-commit": [
    "lint:staged"
  ],
  "author": "",
  "license": "proprietary",
  "dependencies": {
    "aws-sdk": "^2.226.1",
    "axios": "^0.18.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.1.1",
    "css-loader": "^0.28.0",
    "downshift": "^1.31.9",
    "enzyme": "^2.8.1",
    "expect": "^1.20.2",
    "express": "^4.15.2",
    "express-session": "^1.15.6",
    "gulp": "^3.9.1",
    "history": "^4.7.2",
    "knox": "^0.9.2",
    "mocha": "^3.2.0",
    "multer": "^1.3.0",
    "multer-s3": "^2.7.0",
    "node-sass": "^4.5.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.1",
    "q": "^1.5.1",
    "react": "^15.6.2",
    "react-autocomplete": "^1.8.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^15.6.2",
    "react-dropzone": "^4.2.9",
    "react-month-picker": "^1.3.5",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-hash-link": "^1.2.0",
    "react-router-redux": "^4.0.8",
    "reactstrap": "^5.0.0",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "sass-loader": "^6.0.3",
    "semantic-ui": "^2.3.1",
    "semantic-ui-react": "^0.79.1",
    "sequelize": "^4.37.6",
